#:kivy 1.1.0

<MenuScreen>:
    BoxLayout:
        Button:
            text: 'Goto settings'
            on_press: root.manager.current = 'settings'
        Button:
            background_color: [10,1,1,1]
            text: 'Add Red'
            font_size: 40
            on_press: root.my_callback(self,1,'red')

<SettingsScreen>:
    BoxLayout:
        Button:
            background_color: [1,1,10,1]
            text: 'Add Blue'
            font_size: 40
            on_press: root.my_callback(self,1,'blue')
        Button:
            text: 'Back to menu'
            on_press: root.manager.current = 'menu'